package main;

public class MainConditions {

	public static void main(String[] args) {
		compareNumbers(1,3,5);
	}
	
	public static int compareNumbers(int num1, int num2, int num3){
		int mayor=0;
		if (num1>num2 && num1>num3){
			mayor=num1;
		}
		else if (num2>num1 && num2>num3){
			mayor=num2;
		}
		else{
			mayor=num3;
		}
		return mayor;
	}

}
